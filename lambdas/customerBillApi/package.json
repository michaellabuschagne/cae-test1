{
  "name": "customerbillapi",
  "version": "1.0.0",
  "description": "",
  "main": "handler.js",
  "scripts": {
    "start": "node -e 'require(\"./src/handler.js\").queryCustomerBill({body:\"{}\"},null, (a,b) => {console.log(b);})'",
    "test": "npm run start",
    "clean": "rimraf dist *.zip",
    "copy": "babel src package.json package-lock.json --out-dir dist --copy-files",
    "prod": "cross-env NODE_ENV=production npm install --prefix dist",
    "zip": "zip -rXFS9 customerBillApi-v5.zip dist",
    "build": "npm run clean && npm run copy && npm run prod && npm run zip",
    "s3upload:lambda" : "aws s3 cp customerBillApi-v5.zip s3://cae-test1-deployment-artifacts-eu-west-1",
    "s3upload:apispec" : "aws s3 cp src/openapi/customerBillApi-v1.json s3://cae-test1-deployment-artifacts-eu-west-1",
    "s3upload": "npm run s3upload:lambda && npm run s3upload:apispec"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "aws-sdk": "^2.720.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.11.2"
  }
}
